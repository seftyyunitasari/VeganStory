<h3>Users</h3>
    <table>
        <tr>
            <td> Name </td>
            <td> Email </td>
        </tr>
        <% @user.each do |user| %>
        <tr>
            <td> <%= user.name %> </td>
            <td> <%= user.email %> </td>
            <td> <%= button_to "Delete user", user_path(user.id), method: :delete %> </td>
        </tr>
        <% end %>
    </table>